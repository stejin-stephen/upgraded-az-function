{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "HttpTrigger",
    "esModuleInterop": true,
    "allowJs": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "types": ["node", "express"]
  },
  "include": [
    "src/*.ts"
  ],
  "exclude": [
    "node_modules",
  ]
}